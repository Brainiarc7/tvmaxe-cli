#! /bin/bash -x
post=/tmp/tvmaxe-cli/adr
youtube=`cat $post|awk '{print $2}'|sed 's/\./ /g'|awk '{print $2}'`
youtube_m=`cat $post|awk '{print $2}'|sed 's/\./ /g'|awk '{print $2}'`
if [[ "$youtube" = "youtube" ]]||[[ "$youtube_m" = "youtube" ]]; then 
		echo "youtube">/tmp/tvmaxe-cli/yt1
		cat /tmp/tvmaxe-cli/adr|awk '{print $2}'>/tmp/tvmaxe-cli/yt2
			./youtube
			exit 0
else
		./filtrutv
		exit 0
	

fi

